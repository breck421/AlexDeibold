*,*:before,*:after{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box}
html{background:url(../images/bkgd.jpg) no-repeat center center fixed;-webkit-background-size:cover;-moz-background-size:cover;-o-background-size:cover;background-size:cover}
body{background-color:rgba(255,255,255,0);color:#f2f 2 F}
.container{width:100%}
header .navbar{margin:0;color:#262626;background-image:-webkit-gradient(linear, 0 0, 0 100%, from(#aaa9a2), color-stop(65%, #fafbe9), to(#fafbe9));background-image:-webkit-linear-gradient(#aaa9a2, #fafbe9 65%, #fafbe9);background-image:-moz-linear-gradient(top, #aaa9a2, #fafbe9 65%, #fafbe9);background-image:linear-gradient(#aaa9a2, #fafbe9 65%, #fafbe9);background-repeat:no-repeat}header .navbar .active a{background-color:rgba(255,255,255,0)}
footer{position:fixed;bottom:0;left:0;right:0}footer.row{margin:0}
footer .navbar{margin-bottom:0}
nav .navbar-header .navbar-brand{font-size:24px;color:#262626;font-weight:700;text-transform:uppercase}
.transparent{background-color:rgba(255,255,255,0)}
.jumbotron{margin-left:25%;margin-top:18%;width:50%;padding:0}.jumbotron h1{border:0 none;color:#f2f2f2 !important;font-size:123px;font-weight:lighter;line-height:100px;margin:0;padding:0;text-align:center !important;text-decoration:none !important;text-transform:uppercase}
.jumbotron h2{color:#f2f2f2;line-height:0;text-align:right}
